{
    "name": "axstrad/doctrine-extensions",
    "description": "Extensions for Doctrine ORM",
    "type": "library",
    "keywords": ["axstrad", "doctrine", "orm", "extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Kempster",
            "email": "dev@dankempster.co.uk",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "~5.4",
        "gedmo/doctrine-extensions": "~2.3"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "satooshi/php-coveralls": "dev-master"
    },

    "autoload": {
        "psr-4": {
            "Axstrad\\DoctrineExtensions\\": ""
        }
    },

    "minimum-stability": "dev",

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev",
            "dev-develop": "0.2.x-dev"
        }
    },

    "archive": {
        "exclude": ["!vendor", "Tests", "phpunit.xml*", ".gitignore", ".travis.yml"]
    }
}
